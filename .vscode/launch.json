{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "üêç Django: Runserver",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "runserver",
          "--noreload"
        ],
        "django": true,
        "cwd": "${workspaceFolder}",
        "env": {
          "DJANGO_SETTINGS_MODULE": "config.settings"
        },
        "console": "integratedTerminal"
      }
    ]
  }
  